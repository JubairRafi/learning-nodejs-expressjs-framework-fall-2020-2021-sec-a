<!DOCTYPE html>
<html>
<head>
	<title>Edit employer</title>
</head>
<body>
	<form method="post">
		<fieldset>
			<legend>Edit employer</legend>
			<table>
				<tr>
					<td>Employer Name</td>
					<td><input type="text" name="employerName" value="<%=employer.employerName%>"></td>
				</tr>
				<tr>
					<td>Company Name</td>
					<td><input type="text" name="companyName" value="<%=employer.companyName%>"></td>
				</tr>
				<tr>
					<td>contact</td>
					<td><input type="text" name="contact" value="<%=employer.contact%>"></td>
				</tr>
				<tr>
					<td>Username</td>
					<td><input type="text" name="username" value="<%=employer.username%>"></td>
				</tr>
				<tr>
					<td>Password</td>
					<td><input type="password" name="password" value="<%=employer.password%>"></td>
				</tr>
			
				<tr>
					<td></td>
					<td><input type="submit" name="submit" value="Submit"></td>
				</tr>
			</table>
		</fieldset>
	</form>
</body>
</html>